.order-bottom .input-box {
  height: 230rpx;
  width: 690rpx;
  position: relative;
  background: #fff;
}

.order-bottom .input-box .content {
  position: absolute;
  top: 0;
  left: 0;
  display: block;
  background: #fff;
  font-size: 29rpx;
  border: 5px solid #f4f4f4;
  height: 150rpx;
  width: 650rpx;
  padding: 20rpx;
}

.order-bottom .input-box .count {
  position: absolute;
  bottom: 33rpx;
  right: 20rpx;
  display: block;
  height: 30rpx;
  width: 50rpx;
  font-size: 29rpx;
  color: #999;
}
